{
  "name": "awesomewm-example",
  "private": true,
  "version": "0.0.1",
  "description": "usage example of typescript type declarations for awesomewm (4.3) based on typescript-to-lua",
  "main": "src/rc.ts",
  "scripts": {
    "build": "tstl",
    "watch": "tstl --watch",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "eslint '*/**/*.{js,jsx,ts,tsx}' --fix"
  },
  "files": [
    "src/**.ts"
  ],
  "author": "saksmt <smt@saksmt.dev>",
  "license": "MIT",
  "prettier": {
    "trailingComma": "all",
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "proseWrap": "always",
    "tabWidth": 2
  },
  "dependencies": {
    "lua-types": "^2.8.0",
    "awesomewm.4.3.ts.d": "file:../typing"
  },
  "devDependencies": {
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "prettier": "^2.2.0",
    "typescript": "^4.1.3",
    "typescript-to-lua": "^0.37.1"
  }
}
