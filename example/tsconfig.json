{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "types": ["lua-types/5.3", "typescript-to-lua/language-extensions", "awesomewm.4.3.ts.d"],
    "strict": true,
    "rootDir": "src"
  },
  "tstl": {
    "luaTarget": "5.3",
    "noImplicitSelf": true,
    "noHeader": true,
    "luaBundle": "./rc.lua",
    "luaBundleEntry": "./src/rc.ts"
  }
}
